[apply]
  whitespace = fix
[color]
  branch = auto
  diff = auto
  status = auto
  ui = true
[color "branch"]
  current = yellow reverse
  local = yellow
  remote = green
[color "diff"]
  frag = magenta bold
  meta = yellow bold
  new = green bold
  old = red bold
[color "interactive"]
  frag = magenta bold
  meta = yellow bold
  new = green bold
  old = red bold
[color "status"]
  added = yellow
  changed = green
  untracked = cyan
[core]
  editor = vim
  eol = lf
  excludesfile = ~/.gitignore
[gist]
  browse = yes
  extension = rb
  private = no
[github]
  user = gregnostic
[status]
  relativepaths = false
[user]
  email = gregnostic@gmail.com
  name = Greg Hines
